# Aliases for zsh.
# The filename ensures these are loaded last and override other values.

alias apps='sudo /usr/bin/software-center'
alias c=clear
alias d='$HOME/dotfiles/do'
alias g=googler
alias h='$HOME/pyenv/bin/howdoi -n 3 -c'
alias today='gcalcli agenda now 11:59pm --calendar $CALENDAR --detail_location --detail_description'
alias week='gcalcli agenda --calendar $CALENDAR'
